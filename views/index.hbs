<h1>{{title}}</h1>
<table class = "table table-striped table-hover">
	<thead>
		<tr>
			<th>Capability</th>
			<th>ServiceName</th>
			<th>ServiceEndpointUri</th>
			<th>ServiceResourceId</th>
		</tr>
	</thead>
	<tbody>
		{{#each DiscoveryServices.value}}
		<tr>
			<td>{{capability}}</td>
			<td>{{serviceName}}</td>
			<td>{{serviceEndpointUri}}</td>
			<td>{{serviceResourceId}}</td>
		</tr>
		{{/each}}
	</tbody>		
</table>
